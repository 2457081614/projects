<!DOCTYPE html [
<!ENTITY nbsp "&#160;"> 
]>
<f:view xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.org/ui">

	<br />
	<br />
	<div style="width: 100%" id="div">
		<p:accordionPanel id="accordionPanel" onTabShow="updateList()">
			<p:tab title="已创建，未下达" id="unassignedTask">
				<h:form id="taskForm1">
					<p:commandButton id="hidedBtn" binding="#{hidedBtn}"
						update="@form:unassignedTaskListTable" />
					<p:dataTable id="unassignedTaskListTable" var="taskInfo"
						value="#{taskView.unassignedTaskList}" rows="5"
						binding="#{unassignedTaskListTable}"
						sortBy="#{taskInfo.createdDate}"
						filteredValue="#{taskView.filteredTaskList}"
						paginatorPosition="bottom" paginator="true"
						rowsPerPageTemplate="5,10,15,20"
						paginatorTemplate="{CurrentPageReport} 
											{FirstPageLink}
											{PreviousPageLink}
											{PageLinks}
											{NextPageLink}
											{LastPageLink}
											{RowsPerPageDropdown}"
						currentPageReportTemplate="{totalRecords}
									 ({currentPage} of {totalPages})"
						rowKey="#{taskInfo.id}" selection="#{taskView.selectedTaskList}"
						widgetVar="unassignedTaskListTable" rowSelectMode="add">

						<p:column selectionMode="multiple"
							style="width:16px;text-align:center" />

						<f:facet name="header">
							<p:outputPanel style="text-align: left;font-weight:normal">
								<h:outputText value="请输入过滤条件：" />
								<p:inputText id="globalFilter"
									onkeyup="PF('unassignedTaskListTable').filter()"
									style="width:150px" placeholder="过滤" />
								<p:commandButton value="使用模板" immediate="true"
									oncomplete="PF('chooseTaskTemplate').show()"
									update="@form:chooseTaskTemplate"
									icon="fa fa-fw fa-file-text-o" style="margin-left:2%;">
								</p:commandButton>
							</p:outputPanel>
						</f:facet>

						<p:column headerText="任务描述" filterBy="#{taskInfo.taskDescription}"
							filterable="false" filterMatchMode="contains"
							style="width:450px;">
							<p:commandLink title="点击查看此任务的所有问题"
								value="#{taskInfo.taskDescription}"
								style="color:#003a6c;text-decoration: underline;"
								process="@this" oncomplete="alert('待完善...')" immediate="true">
								<f:setPropertyActionListener value="#{taskInfo}"
									target="#{taskView.taskInfo}" />
							</p:commandLink>
						</p:column>


						<p:column headerText="创建时间" filterBy="#{taskInfo.createdDate}"
							filterMatchMode="contains" filterable="false">
							<h:outputText value="#{taskInfo.createdDate}" />
						</p:column>

						<p:column headerText="任务阶段" filterBy="#{taskInfo.stage}"
							filterMatchMode="contains" filterable="false">
							<h:outputText value="#{taskInfo.stage}" />
						</p:column>

						<p:column headerText="分配客户">
							<p:commandButton title="给任务分配客户" value="分配客户"
								oncomplete="PF('dlg_assignCustomer').show()"
								icon="fa fa-fw fa-user-plus" update="@form:dlg_assignCustomer">
								<f:setPropertyActionListener value="#{taskInfo}"
									target="#{taskView.task}" />
							</p:commandButton>
						</p:column>

						<p:column headerText="删除任务">
							<p:commandButton title="删除此任务" value="删除" icon="ui-icon-trash"
								ajax="true" oncomplete="PF('dlg_confirmToDeleteOne').show()">
								<f:setPropertyActionListener value="#{taskInfo}"
									target="#{taskView.task}" />
							</p:commandButton>
						</p:column>
					</p:dataTable>


					<!-- 确认删除对话框 -->
					<!-- 确认删除对话框 -->
					<!-- 确认删除对话框 -->
					<!-- 确认删除对话框 -->
					<!-- 确认删除对话框 -->
					<p:dialog id="dlg_confirmToDeleteOne" header="请确认"
						widgetVar="dlg_confirmToDeleteOne" modal="true"
						showEffect="bounce" hideEffect="fade" resizable="false"
						width="250" height="60">

						<table>
							<tr>
								<h:outputText value="确认删除吗?"
									style="font-size:20px;color:#003a6c;text-align:center;" />
							</tr>
							<tr>
								<td><p:commandButton value="Cancel"
										oncomplete="PF('dlg_confirmToDeleteOne').hide()"
										icon="fa fa-fw fa-remove" immediate="true">
									</p:commandButton></td>
								<td><p:commandButton value="OK" style="margin-left:13%;"
										action="#{taskView.deleteTask}"
										update="@form:unassignedTaskListTable"
										oncomplete="PF('dlg_confirmToDeleteOne').hide()"
										icon="ui-icon-check">
									</p:commandButton></td>
							</tr>
						</table>
					</p:dialog>
					<!--    分配客户 -->
					<!--    分配客户 -->
					<!--    分配客户 -->
					<!--    分配客户 -->
					<!--    分配客户 -->
					<p:dialog header="分配客户" widgetVar="dlg_assignCustomer" modal="true"
						id="dlg_assignCustomer" showEffect="blind" hideEffect="clip"
						resizable="false" width="50%">
						<p:dataTable id="assignCustomerListTable" var="customer"
							value="#{customerView.customerList}" rows="7"
							binding="#{assignCustomerListTable}" sortBy="#{customer.id}"
							filteredValue="#{customerView.filteredCustomerList}"
							paginatorPosition="bottom" paginator="true"
							rowsPerPageTemplate="5,10,15,20"
							paginatorTemplate="{CurrentPageReport} 
											{FirstPageLink}
											{PreviousPageLink}
											{PageLinks}
											{NextPageLink}
											{LastPageLink}
											{RowsPerPageDropdown}"
							currentPageReportTemplate="{totalRecords}
									 ({currentPage} of {totalPages})"
							rowKey="#{customer.id}"
							selection="#{customerView.selectedCustomerList}"
							widgetVar="assignCustomerListTable" rowSelectMode="add">

							<p:column selectionMode="multiple"
								style="width:16px;text-align:center" />

							<f:facet name="header">
								<p:outputPanel style="text-align: left;font-weight:normal">
									<h:outputText value="请输入过滤条件：" />
									<p:inputText id="globalFilter"
										onkeyup="PF('assignCustomerListTable').filter()"
										style="width:150px" placeholder="过滤" />
								</p:outputPanel>
							</f:facet>
							<p:column headerText="姓名" filterBy="#{customer.name}"
								filterMatchMode="contains" filterable="false">
								<h:outputText value="#{customer.name}" />
							</p:column>
							<p:column headerText="学院" filterBy="#{customer.academy}"
								filterMatchMode="contains" filterable="false">
								<h:outputText value="#{customer.academy}" />
							</p:column>
							<p:column headerText="地址" filterBy="#{customer.address}"
								filterMatchMode="contains" filterable="false">
								<h:outputText value="#{customer.address}" />
							</p:column>
						</p:dataTable>
						<br />
						<p:separator />

						<p:commandButton value="返回" icon="ui-icon-arrowreturnthick-1-w"
							oncomplete="PF('dlg_assignCustomer').hide()"
							style="margin-left: 30%" />
						<p:commandButton value="下一步" icon="ui-icon-arrowthick-1-s"
							iconPos="right"
							action="#{taskView.validateSelectedCustomerList(1)}"
							update="@form:dlg_assignParttimeStaff" style="margin-left:18%" />
					</p:dialog>
					<!-- 	分配兼职 -->
					<!-- 	分配兼职 -->
					<!-- 	分配兼职 -->
					<!-- 	分配兼职 -->
					<!-- 	分配兼职 -->
					<p:dialog header="分配兼职" widgetVar="dlg_assignParttimeStaff"
						modal="true" id="dlg_assignParttimeStaff" showEffect="blind"
						hideEffect="size" resizable="false" width="50%">
						<p:dataTable id="assignParttimeStaffListTable" var="parttimeStaff"
							value="#{parttimeStaffView.parttimeStaffList}" rows="5"
							binding="#{assignParttimeStaffListTable}"
							sortBy="#{parttimeStaff.id}"
							filteredValue="#{parttimeStaffView.filteredParttimeStaffList}"
							paginatorPosition="bottom" paginator="true"
							rowsPerPageTemplate="5,10,15,20"
							paginatorTemplate="{CurrentPageReport} 
											{FirstPageLink}
											{PreviousPageLink}
											{PageLinks}
											{NextPageLink}
											{LastPageLink}
											{RowsPerPageDropdown}"
							currentPageReportTemplate="{totalRecords}
									 ({currentPage} of {totalPages})"
							rowKey="#{parttimeStaff.id}"
							selection="#{taskView.selectedParttimeStaffInNotUsingTemplate}"
							widgetVar="assignParttimeStaffListTable" selectionMode="single">

							<p:ajax event="rowSelect" listener="#{taskView.onRowSelect}"
								update="@form:assignParttimeStaffListTable" />

							<p:column selectionMode="single"
								style="width:16px;text-align:center" />

							<f:facet name="header">
								<p:outputPanel style="text-align: left;font-weight:normal">
									<h:outputText value="请输入过滤条件：" />
									<p:inputText id="globalFilter"
										onkeyup="PF('assignParttimeStaffListTable').filter()"
										style="width:150px" placeholder="过滤" />

								</p:outputPanel>
							</f:facet>
							<p:column headerText="姓名" filterBy="#{parttimeStaff.name}"
								filterMatchMode="contains" filterable="false">
								<h:outputText value="#{parttimeStaff.name}" />
							</p:column>
							<p:column headerText="联系电话" filterBy="#{parttimeStaff.tel}"
								filterMatchMode="contains" filterable="false">
								<h:outputText value="#{parttimeStaff.tel}" />
							</p:column>

							<p:column headerText="QQ" filterBy="#{parttimeStaff.qq}"
								filterMatchMode="contains" filterable="false">
								<h:outputText value="#{parttimeStaff.qq}" />
							</p:column>

							<p:column headerText="住址" filterBy="#{parttimeStaff.address}"
								filterMatchMode="contains" filterable="false">
								<h:outputText value="#{parttimeStaff.address}" />
							</p:column>
						</p:dataTable>
						<br />
						<div id="div_plannedFinishedDate"
							style="margin-left: 50%; margin-top: 2%;">
							<h:outputText style="color:#003a6c;font-size:16px;"
								value="计划完成日期:" />
							<p:calendar effect="blind" style="font-size:16px;"
								placeholder="点击选择日期" locale="zh_CN"
								mindate="#{taskView.currentDate}" pattern="yyyy-MM-dd"
								navigator="true" yearRange="c:c+5" lang="zh_CN"
								value="#{taskView.plannedFiniedDateInNotUsingTemplate}"
								readonlyInput="true" immediate="true">
							</p:calendar>
						</div>
						<p:separator />
						<br />
						<p:commandButton value="上一步" icon="ui-icon-arrowthick-1-n"
							oncomplete="PF('dlg_assignParttimeStaff').hide(),PF('dlg_assignCustomer').show()"
							style="margin-left: 30%" />
						<p:commandButton value="确认" icon="ui-icon-check"
							oncomplete="PF('dlg_confirmAssign').show()"
							style="margin-left: 18%" />
					</p:dialog>


					<!-- 一次性分配多个客户和一个兼职 -->
					<p:dialog id="dlg_confirmAssign" header="请确认"
						widgetVar="dlg_confirmAssign" modal="true" showEffect="bounce"
						hideEffect="fade" resizable="false" width="230" height="60">
						<table>
							<tr>
								<h:outputText value="确认分配任务吗?"
									style="font-size:20px;color:#003a6c;text-align:center;" />
							</tr>
							<tr>
								<td><p:commandButton value="Cancel"
										oncomplete="PF('dlg_confirmAssign').hide()"
										icon="ui-icon-cancel">
									</p:commandButton></td>
								<td><p:commandButton value="OK" style="margin-left:15%;"
										action="#{taskView.assignTasks}"
										update="@form:unassignedTaskListTable"
										oncomplete="PF('dlg_confirmAssign').hide(),PF('dlg_assignParttimeStaff').hide()"
										icon="ui-icon-check">
									</p:commandButton></td>
							</tr>
						</table>
					</p:dialog>

					<!-- 选择任务模板对话框 -->
					<!-- 选择任务模板对话框 -->
					<!-- 选择任务模板对话框 -->
					<!-- 选择任务模板对话框 -->
					<p:dialog id="chooseTaskTemplate" header="请选择任务模板"
						widgetVar="chooseTaskTemplate" modal="true" showEffect="blind"
						hideEffect="clip" resizable="false" width="800px">
						<p:dataTable id="taskTemplatesTable" var="taskTemplate"
							value="#{taskView.taskTemplates}" rows="5"
							binding="#{taskTemplatesTable}"
							sortBy="#{taskTemplate.createdDate}"
							filteredValue="#{taskView.filteredTaskList}"
							paginatorPosition="bottom" paginator="true"
							rowsPerPageTemplate="5,10,15,20"
							paginatorTemplate="{CurrentPageReport} 
											{FirstPageLink}
											{PreviousPageLink}
											{PageLinks}
											{NextPageLink}
											{LastPageLink}
											{RowsPerPageDropdown}"
							currentPageReportTemplate="{totalRecords}
									 ({currentPage} of {totalPages})"
							rowKey="#{taskTemplate.id}" selectionMode="single"
							widgetVar="taskTemplatesTable">

							<p:ajax event="rowSelect"
								listener="#{taskView.onSelectTaskTemplate}"
								update="@form:taskTemplatesTable" />

							<p:column selectionMode="single"
								style="width:16px;text-align:center" />

							<f:facet name="header">
								<p:outputPanel style="text-align: left;font-weight:normal">
									<h:outputText value="请输入过滤条件：" />
									<p:inputText id="globalFilter"
										onkeyup="PF('taskTemplatesTable').filter()"
										style="width:150px" placeholder="过滤" />
								</p:outputPanel>
							</f:facet>

							<p:column headerText="任务描述"
								filterBy="#{taskTemplate.taskDescription}" filterable="false"
								filterMatchMode="contains" style="width:450px;">
								<h:outputText value="#{taskTemplate.taskDescription}"
									style="color:#003a6c;" process="@this" />
							</p:column>

							<p:column headerText="任务阶段" filterBy="#{taskTemplate.stage}"
								filterMatchMode="contains" filterable="false">
								<h:outputText value="#{taskTemplate.stage}" />
							</p:column>

							<p:column headerText="创建时间"
								filterBy="#{taskTemplate.createdDate}"
								filterMatchMode="contains" filterable="false">
								<h:outputText value="#{taskTemplate.createdDate}" />
							</p:column>
							<p:column headerText="任务模板的问题">
								<p:commandButton value="查看" icon="ui-icon-search">
								</p:commandButton>
							</p:column>
						</p:dataTable>
						<br />
						<p:separator />
						<p:commandButton value="返回" icon="ui-icon-arrowreturnthick-1-w"
							oncomplete="PF('chooseTaskTemplate').hide()"
							style="margin-left: 30%;margin-top:2%;" />
						<p:commandButton value="下一步" icon="ui-icon-arrowthick-1-s"
							iconPos="right" action="#{taskView.validateSelectedTaskTemplate}"
							update="@form:useTaskTemplate" style="margin-left:18%" />
					</p:dialog>

					<!-- 使用任务模板对话框 -->
					<!-- 使用任务模板对话框 -->
					<!-- 使用任务模板对话框 -->
					<p:dialog id="useTaskTemplate" widgetVar="useTaskTemplate"
						modal="true" showEffect="blind" hideEffect="clip"
						resizable="false" width="740" height="500" header="正在使用任务模板...">
						<h:outputText
							style="float:left;margin-left:2%;color:red;font-size:14px;"
							value="温馨提示:可在任务模板的基础上进行修改，修改不会对任务模板造成影响" />
						<br />
						<p:outputPanel id="task_description" style="margin-top:10px;">
							<table class="common_tab">
								<tr>
									<th
										style="width: 15%; float: left; margin-left: 2%; border-radius: 5px;"><span
										style="font-size: 16px;">任务描述:</span></th>
								</tr>
								<tr>
									<td><p:inputTextarea rows="4" placeholder="请输入对任务的描述"
											value="#{taskView.newTask.taskDescription}"
											style="font-size:16px;margin-left:2%; border: solid Dark 1px;border-radius:5px; width:95%;" />
									</td>
								</tr>
							</table>
						</p:outputPanel>

						<p:outputPanel id="add_question">
							<br />
							<br />
							<table class="common_tab">
								<tr>
									<th
										style="width: 15%; float: left; margin-left: 2%; border-radius: 5px;"><span
										style="font-size: 16px;">添加问题:</span></th>
								</tr>
							</table>
							<p:dataTable id="newQuestionListTableInTm" var="question"
								value="#{questionView.newQuestionList}"
								rows="#{questionView.newQuestionList.size()}"
								binding="#{newQuestionListTableInTm}" paginator="false"
								widgetVar="newQuestionListTableInTm" style="width:96%;">
								<p:column headerText="题号" styleClass="nameColumn">
									<h:outputText
										value="#{questionView.newQuestionList.indexOf(question)+1}" />
								</p:column>
								<p:column headerText="问题内容">
									<p:inputTextarea rows="2" autoResize="false"
										style="width:100%;font-size:16px;"
										placeholder="请输入问题#{questionView.newQuestionList.indexOf(question)+1}的内容,若要手动编辑，可在选择问题后，按退格键删除问题内容，然后编辑"
										value="#{question.questionContent}"
										onfocus="showTable(this),passQuestionNo('#{questionView.newQuestionList.indexOf(question)}')" />
									<p:remoteCommand name="getParams"
										actionListener="#{questionView.getParams}" />
								</p:column>
								<p:column headerText="清除问题内容" styleClass="modificationColumn">
									<p:commandButton value="清除" style="border-radius:3px;"
										process="@this" update="@form:newQuestionListTableInTm"
										icon="ui-icon-trash"
										action="#{questionView.clearQuestionContent}">
										<f:setPropertyActionListener value="#{question}"
											target="#{questionView.newQuestion}" />
									</p:commandButton>
								</p:column>
							</p:dataTable>
							<table style="border: 1px;">
								<tr>
									<p:graphicImage value="/images/plus.png"
										onclick="lengthenTable()" />
								</tr>
								<p:remoteCommand name="lengthenList"
									update="@form:newQuestionListTableInTm"
									actionListener="#{questionView.lengthenList}" />
							</table>
						</p:outputPanel>
						<br />
						<p:separator style="width:100%; margin-top:10px;" />
						<div style="margin-top: 20px; margin-bottom: 10px;">
							<p:commandButton value="上一步" icon="ui-icon-arrowthick-1-n"
								immediate="true"
								oncomplete="PF('useTaskTemplate').hide(),PF('chooseTaskTemplate').show()"
								style="height:35px;width:100px;border-radius:3px;margin-left:30%">
							</p:commandButton>
							<p:commandButton value="下一步" id="comfirmTaskBtn"
								icon="ui-icon-arrowthick-1-s" immediate="true"
								update="@form:assignCustomer"
								oncomplete="PF('useTaskTemplate').hide(),PF('assignCustomer').show()"
								style="height:35px;width:100px;margin-left:10%;border-radius:3px;">
							</p:commandButton>
						</div>
					</p:dialog>

					<!--使用任务模板第三步，选择客户-->
					<!--使用任务模板第三步，选择客户-->
					<!--使用任务模板第三步，选择客户-->
					<!--使用任务模板第三步，选择客户-->
					<p:dialog header="分配客户" widgetVar="assignCustomer" modal="true"
						id="assignCustomer" showEffect="blind" hideEffect="clip"
						resizable="false" width="50%">
						<p:dataTable id="assignCustomerListTable2" var="customer"
							value="#{customerView.customerList}" rows="7"
							binding="#{assignCustomerListTable2}" sortBy="#{customer.id}"
							filteredValue="#{customerView.filteredCustomerList}"
							paginatorPosition="bottom" paginator="true"
							rowsPerPageTemplate="5,10,15,20"
							paginatorTemplate="{CurrentPageReport} 
											{FirstPageLink}
											{PreviousPageLink}
											{PageLinks}
											{NextPageLink}
											{LastPageLink}
											{RowsPerPageDropdown}"
							currentPageReportTemplate="{totalRecords}
									 ({currentPage} of {totalPages})"
							rowKey="#{customer.id}"
							selection="#{customerView.selectedCustomersForTaskTemplate}"
							widgetVar="assignCustomerListTable2" rowSelectMode="add">

							<p:column selectionMode="multiple"
								style="width:16px;text-align:center" />

							<f:facet name="header">
								<p:outputPanel style="text-align: left;font-weight:normal">
									<h:outputText value="请输入过滤条件：" />
									<p:inputText id="globalFilter"
										onkeyup="PF('assignCustomerListTable2').filter()"
										style="width:150px" placeholder="过滤" />
								</p:outputPanel>
							</f:facet>
							<p:column headerText="姓名" filterBy="#{customer.name}"
								filterMatchMode="contains" filterable="false">
								<h:outputText value="#{customer.name}" />
							</p:column>
							<p:column headerText="学院" filterBy="#{customer.academy}"
								filterMatchMode="contains" filterable="false">
								<h:outputText value="#{customer.academy}" />
							</p:column>
							<p:column headerText="地址" filterBy="#{customer.address}"
								filterMatchMode="contains" filterable="false">
								<h:outputText value="#{customer.address}" />
							</p:column>
						</p:dataTable>
						<br />
						<p:separator />

						<p:commandButton value="上一步" icon="ui-icon-arrowthick-1-n"
							oncomplete="PF('assignCustomer').hide(),PF('useTaskTemplate').show()"
							style="margin-left: 30%" />
						<p:commandButton value="下一步" icon="ui-icon-arrowthick-1-s"
							iconPos="right"
							action="#{taskView.validateSelectedCustomerList(2)}"
							update="@form:assignParttimeStaff" style="margin-left:18%" />
					</p:dialog>

					<!--使用任务模板第四步，选择兼职-->
					<!--使用任务模板第四步，选择兼职-->
					<!--使用任务模板第四步，选择兼职-->
					<p:dialog header="分配兼职" widgetVar="assignParttimeStaff"
						modal="true" id="assignParttimeStaff" showEffect="blind"
						hideEffect="size" resizable="false" width="50%">
						<p:dataTable id="assignParttimeStaffListTable2"
							var="parttimeStaff"
							value="#{parttimeStaffView.parttimeStaffList}" rows="5"
