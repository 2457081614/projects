	<script type="text/javascript">
		$(document).ready(function() {
			$("[id='#{hidedBtn.clientId}']").hide();
			$("[id='#{hidedBtn2.clientId}']").hide();
			$("[id='#{hiddenBtn.clientId}']").hide();
			$("[id='#{hidedBtn.clientId}']").click();
		});

		function updateList() {
			$("[id='#{hidedBtn2.clientId}']").click();
		}

		function passQuestionNo(obj) {
			getParams([ {
				name : 'qno',
				value : obj
			} ]);
		}
		function lengthenTable() {
			lengthenList();
		}
		function showTable(obj) {
			if (obj == null || obj.value == null || obj.value.length == 0) {
				PF('dlg_chooseQuestionInTm').show();
				$("[id='#{hiddenBtn.clientId}']").click();
			}
		}
	</script>
